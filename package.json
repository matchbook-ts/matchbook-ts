{
  "name": "@matchbook/ts",
  "author": "Orion Kindel",
  "version": "0.2.0",
  "license": "MIT",
  "repository": "https://github.com/matchbook-ts/matchbook-ts",
  "homepage": "https://github.com/matchbook-ts/matchbook-ts",
  "bugs": "https://github.com/matchbook-ts/matchbook-ts/issues",
  "types": "./lib/src/index.d.ts",
  "main": "./lib/src/index.js",
  "scripts": {
    "test": "nyc ava",
    "test:debug": "nyc ava debug --break --serial",
    "tdd": "nyc ava --watch",
    "check": "gts check src/**/*.ts && gts check test/**/*.ts",
    "compile": "tsc",
    "fix": "gts fix src/**/*.ts && gts fix test/**/*.ts",
    "prepare": "npm run compile",
    "posttest": "npm run check"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/node": "^10.17.19",
    "@types/sinon": "^7.5.2",
    "ava": "^3.7.0",
    "chai": "^4.2.0",
    "gts": "^2.0.0",
    "nyc": "^15.0.1",
    "sinon": "^9.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "optionalDependencies": {},
  "ava": {
    "files": [
      "./test/**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "nyc": {
    "all": true,
    "reporter": "lcov"
  }
}
